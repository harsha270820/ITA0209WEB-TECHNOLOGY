<script>
function jsFun()
{
  var valuee, element;
  valuee = parseInt(document.getElementById("num").value);
  if(valuee)
  {
    if(valuee <20)
      document.getElementById("paraTwo").innerHTML = "Less than 20";
    else
      document.getElementById("paraTwo").innerHTML = "Greater than 20";
  }
}
</script>
